<div class="jumbotron"> <h2>Welcome to St.Joseph's Alumini Portal</h2> </div>
<center><h1>LOG_IN</h1></center>
<div class= "container">
<div class="form-group">
<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
  <div class="field">
    <%= f.label :rollno %><br />
    <%= f.text_field :regno,placeholder: "Register_No",class: "form-control", autofocus: true%>
  </div>
    <div class="field">
    <%= f.label :email %><br />
    <%= f.email_field :email,placeholder: "Email Id",class: "form-control",autofocus: true %>
  </div>

<div class="field">
    <%= f.label :password %><br />
    <%= f.password_field :password,placeholder: "Password",class: "form-control", autocomplete: "off" %>
  </div>
   

  <% if devise_mapping.rememberable? -%>
    <div class="field">
      <%= f.check_box :remember_me %>
      <%= f.label :remember_me %>
    </div>
  <% end -%>

  <div class="actions">
    <%= f.submit "Log in",class: "btn btn-success"%>
  </div>
<% end %>
</div>
</div>
<%= link_to new_student_registration_path, class: "btn btn-default" do %>
    <i class="glyphicon glyphicon-hand-right"></i> Sign_Up
<% end %>
<%= render "devise/shared/links" %>
